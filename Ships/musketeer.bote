	"lines": {
		"skin": {
			"1": {
				"skin_id": "huoqiangshou",
				"id": "0",
				"label": "Musketeer",
				"dialogue": {
					"1": {
						"event": "Ship Description",
						"media": "",
						"chinese": "M级驱逐舰—火枪手，舷号86",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "M級駆逐艦・マスケティーア（G86)",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "M-class destroyer – Musketeer (G86.)",
						"englishNote": ""
					},
					"2": {
						"event": "Biography",
						"media": "profile",
						"chinese": "皇家M级驱逐舰火枪手号，参与过北角海战！指挥官，让我砰—砰—地把你的坏心情全部消灭吧！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ロイヤルネイビーM級駆逐艦マスケティーア！北岬沖海戦参加歴あり！指揮官、このマスケティーアがパーン……パーンっとあなたの暗いココロ撃ち抜いちゃうぞ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I'm the Royal Navy M-class destroyer Musketeer! I was in the Battle of the North Cape! Commander, little Musketeer is gonna shoot straight through your dreary heart with a bang bang!",
						"englishNote": ""
					},
					"3": {
						"event": "Acquisition",
						"media": "get",
						"chinese": "M级驱逐舰火枪手号，嘿嘿~我就知道今天会遇到什么好事~指挥官，港口在哪个方向？这边？还是这边！？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "私はM級駆逐艦、マスケティーア！へっへー、今日はいいことがあると思ったんだ～指揮官、母港はどこかな？こっち？それともこっち！？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I am the M-class Destroyer, Musketeer! Hehe... I knew something good was going to happen today~ Commander, can you point me to where the port is? Here? Or over here?!",
						"englishNote": ""
					},
					"4": {
						"event": "Login",
						"media": "login",
						"chinese": "指挥官，早上好！今天的天气也很棒呀！砰—砰—！嘿嘿~我情绪总是莫名其妙的高涨呢",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官おっはよー！今日もいい天気～カチャカチャ…バン！ひゅ～なんだか私、いっつもハイテンションだよね！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Morning, Commander! Nice weather today! Click, click... bang! I'm not sure why, but I'm always brimming with energy!",
						"englishNote": ""
					},
					"5": {
						"event": "Details",
						"media": "detail",
						"chinese": "指挥官，和我在一起的时候快乐吗？……啊，对不起，请把这个无聊的问题忘掉吧——",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官、私と一緒にいると楽しい？……あーごめん、今の忘れて？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Is it fun to hang out with me, Commander? ...Ah, sorry, never mind that.",
						"englishNote": ""
					},
					"6": {
						"event": "Main 1",
						"media": "main_1",
						"chinese": "今天一定会有好事发生的，嗯，我有这样的预感",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "今日はいいことが起きるよ～うん、そんな気がする！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Something nice is gonna happen today~ That's what I'm feeling!",
						"englishNote": ""
					},
					"7": {
						"event": "Main 2",
						"media": "main_2",
						"chinese": "我总是在笑？嘿嘿~因为来到这里后我一直很高兴啊",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "いつも笑ってるって？へへへーだってここに来てからいっつも楽しいもん！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Why am I always smiling? Hehehe, because I've been having so much fun ever since I came here!",
						"englishNote": ""
					},
					"8": {
						"event": "Main 3",
						"media": "main_3",
						"chinese": "战斗中受的伤？嘿嘿~一点都不痛啦～",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "戦闘中に負ったケガ？へへへー全然痛くないよ？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "The injuries I've sustained from combat? Hehehe, they don't hurt at all.",
						"englishNote": ""
					},
					"9": {
						"event": "Touch",
						"media": "touch_1",
						"chinese": "嘿嘿~心中又充满干劲啦~",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "へへへ～やる気、リロード完了～！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Hehehe~! You've reloaded me with energy!",
						"englishNote": ""
					},
					"10": {
						"event": "Touch (Special)",
						"media": "touch_2",
						"chinese": "哎呀，我也不讨厌……嗯，不讨厌啦！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "こういうのキライじゃないけど……うん…キライじゃないよ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "It's not like I *dislike* it, but... Yeah, I actually don't dislike it!",
						"englishNote": ""
					},
					"11": {
						"event": "Mission",
						"media": "task",
						"chinese": "一鼓作气解决它们！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "一気にやっちゃうぞ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Finish them all in one go!",
						"englishNote": ""
					},
					"12": {
						"event": "Mission Complete",
						"media": "mission_complete",
						"chinese": "嘿嘿，见到钱高兴可是本能，本能！指挥官也更高兴一点嘛~",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "へへへ～お金を見て嬉しくなるのは人の本能！そう！指揮官も笑って笑って～",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Hehehe~ It's just human instinct to smile at the sight of money! Yeah, that's it! Smile, Commander~!",
						"englishNote": ""
					},
					"13": {
						"event": "Mail",
						"media": "mail",
						"chinese": "我就说今天会有好事发生啦！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ふんふん♪だから今日はいいことがあるって！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Heehee~! See? I told you something nice would happen today!",
						"englishNote": ""
					},
					"14": {
						"event": "Return to Port",
						"media": "home",
						"chinese": "指挥官，请看看我新编的军舞！放心吧，这次火枪里没有实弹！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官、私の新しいマーチ芸を見て！大丈夫！今度は空砲だから！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Take a gander at my new marching act, Commander! Relax, I'm only using blanks this time!",
						"englishNote": ""
					},
					"15": {
						"event": "Commission Complete",
						"media": "expedition",
						"chinese": "哇哦，好多奖励！多到我都想试试石油浴了！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "わあ！ボーナスがいっぱい！燃料でお風呂を作ってみちゃったり！？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Wooow! Look at those mission rewards! We could fill a bathtub with all this oil!",
						"englishNote": ""
					},
					"16": {
						"event": "Enhancement",
						"media": "upgrade",
						"chinese": "瞄准～射击——砰砰砰砰砰！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "狙ってぇ～撃てーーパン！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Take aim~! Fire! Bang!",
						"englishNote": ""
					},
					"17": {
						"event": "Flagship",
						"media": "warcry",
						"chinese": "呀吼~那么，以枪击为号——",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "やっほーじゃあー射撃よーい！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "How-dee-doooo! I'm gonna shoot yooou!",
						"englishNote": ""
					},
					"18": {
						"event": "Victory",
						"media": "mvp",
						"chinese": "射穿你的心☆~嘿嘿，我早想说一次这句台词了！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "あなたのハート、ぶち抜いちゃうよ～へっへーいちど言ってみたかったんだ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I'm gonna punch a hole straight through your heart~! Heheh, I've always wanted to say that!",
						"englishNote": ""
					},
					"19": {
						"event": "Defeat",
						"media": "lose",
						"chinese": "悄悄地~灰溜溜地~撤出战场~♫",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ひっそりこっそり脱出～♫っと",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Time to make a sneaky exit~♫",
						"englishNote": ""
					},
					"20": {
						"event": "Skill",
						"media": "skill",
						"chinese": "转轮齐射，准备就绪！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "装填完了～発射よーい！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Locked and loaded~! Ready to fire!",
						"englishNote": ""
					},
					"21": {
						"event": "Affinity (Upset)",
						"media": "feeling1",
						"chinese": "赶快变回……原来那个指挥官啊……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "はやく……元の指揮官に戻って……",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Come on... Return to your former self, Commander...",
						"englishNote": ""
					},
					"22": {
						"event": "Affinity (Stranger)",
						"media": "feeling2",
						"chinese": "无论遇到什么都要想积极的方面，乐观、再乐观一点，嗯，我必须得这样才行！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "どんなことがあっても前向き・陽気・ポジティブ！うん、そうでなくっちゃね！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "No matter what life throws at you, always be optimistic, happy, and positive! You just gotta be!",
						"englishNote": ""
					},
					"23": {
						"event": "Affinity (Friendly)",
						"media": "feeling3",
						"chinese": "和指挥官在一起，无论什么时候都很快乐~",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官といるといつも楽しいよ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I always have tons of fun hanging out with you!",
						"englishNote": ""
					},
					"24": {
						"event": "Affinity (Like)",
						"media": "feeling4",
						"chinese": "比起为无穷无尽的烦恼焦虑郁闷，果然还是打起精神快乐地度过每一天比较好吧，呐，指挥官也这么觉得吧？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "悩むことより、やっぱり楽しく生きたほうがいいよね！ねえ、指揮官もそう思うでしょ？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "We should all forget our worries and live to have fun! Don't you think so too, Commander?",
						"englishNote": ""
					},
					"25": {
						"event": "Affinity (Love)",
						"media": "feeling5",
						"chinese": "遇见指挥官后，我也变了很多呢，不过……最本质的地方我始终是我哦！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官と出会ってからなんか色々あったね…でも、本当の私はなーにも変わってないよ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "So much has happened since we first met... But I haven't changed a bit!",
						"englishNote": ""
					},
					"26": {
						"event": "Pledge",
						"media": "propose",
						"chinese": "谢谢你，指挥官！与至今为止你给予我的那些相比，这个是最珍贵的！呐、呐，指挥官，我也给你留下了同样珍贵的快乐回忆了吗？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官ありがとう！今までもらったものの中で一番大事なものだよ！ねえねえ、私も指揮官に楽しい思い出をあげちゃったのかな？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Thank you, Commander! This is the best thing I've ever gotten! Hey, tell me: have I given you some good memories, too?",
						"englishNote": ""
					}
				}
			}
