                    "1" : {
                        "event" : "Ship Description",
                        "media" : "",
                        "chinese" : "恰巴耶夫级轻巡洋舰—恰巴耶夫",
                        "chineseTL" : "",
                        "chineseNote" : "",
                        "japanese" : "チャパエフ級巡洋艦――チャパエフ",
