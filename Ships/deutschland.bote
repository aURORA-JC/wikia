					"16": {
						"event": "Enhancement",
						"media": "upgrade",
						"chinese": "虽然是低等动物，不过很懂事嘛",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "下等生物の分際でよく分かってるじゃない",
						"japaneseTL": "",
