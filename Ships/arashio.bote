					"1" : {
						"event" : "Ship Description",
						"media" : "",
						"chinese" : "朝潮级驱逐舰四番舰—荒潮",
						"chineseTL" : "",
						"chineseNote" : "",
						"japanese" : "朝潮型駆逐艦四番艦・荒潮",
