					},
					"19": {
						"event": "Defeat",
						"media": "lose",
						"chinese": "请退下吧，否则我将赌上性命……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "お引きください。でなければ私の命を賭けても……",
						"japaneseTL": "",
