					"20": {
						"event": "Skill",
						"media": "skill",
						"chinese": "全部斩断吧！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "全て断ち切る！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I'll cut you all down!",
						"englishNote": ""
					},
					"21": {
						"event": "Low HP",
						"media": "hp",
						"chinese": "现在正是考验我等的时刻！继续冲吧！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "私たちが試される時です！――進めええ！！",
						"japaneseTL": "",
