					"16": {
						"event": "Enhancement",
						"media": "upgrade",
						"chinese": "您的青睐定当得到回报",
						"chineseTL": "Your good graces will definitely be repaid.",
						"chineseNote": "",
						"japanese": "お引き立ていただきありがとうございます。必ずお報いいたします",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Your kindness is highly appreciated. I swear to repay it one day.",
						"englishNote": ""
					},
					"17": {
						"event": "Flagship",
						"media": "warcry",
						"chinese": "诸位，不要掉队了！",
						"chineseTL": "Everyone, don't fall behind!",
						"chineseNote": "",
						"japanese": "みんな、ついてきて！",
						"japaneseTL": "",
						"japaneseNote": "",
