					"18": {
						"event": "Victory",
						"media": "mvp",
						"chinese": "呼……再稍微努力一下吧",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ふぅ……もう少し努力してやろうか",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Phew... Let's work a bit harder!",
