					"17": {
						"event": "Enhancement",
						"media": "upgrade",
						"chinese": "如果能够早些变强，或许……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "もっと早く強くなっていれば、もしかしたら……",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "If only I was able to become this strong sooner, then perhaps...",
						"englishNote": ""
					},
					"18": {
						"event": "Flagship",
						"media": "warcry",
						"chinese": "前辈们，现在逸仙也扛起了这副担子",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "先輩たち、この大任は今、逸仙が担っていますわ",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Ancestors... Yat Sen shall now do her part as well.",
						"englishNote": ""
