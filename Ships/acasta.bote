						"chineseTL": "",
						"chineseNote": "",
						"japanese": "うぅ……元の服に戻したほうがいいかな…",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "*Sigh*... Maybe I should swap back to my old clothes...",
						"englishNote": ""
