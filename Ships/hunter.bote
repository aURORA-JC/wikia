	"lines": {
		"skin": {
			"1": {
				"skin_id": "lieren",
				"id": "0",
				"label": "ハンター",
				"dialogue": {
					"1": {
						"event": "Ship Description",
						"media": "",
						"chinese": "H级驱逐舰—猎人，舷号H35",
						"chineseTL": "H-class Destroyer - Hunter. Pennant number: H35",
						"chineseNote": "",
						"japanese": "H級駆逐艦・ハンター（H35)",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"2": {
						"event": "Biography",
						"media": "profile",
						"chinese": "我是皇家海军H级的猎人，参与纳尔维克海战时输给了铁血的驱逐……猎人被猎物咬死，大概也是一种宿命吧",
						"chineseTL": "I am H class's Hunter. At narvik I lost to a submarine.. a hunter gets hunted. How ironic.",
						"chineseNote": "",
						"japanese": "ロイヤルネイビーHクラス、ハンター。ナルヴィク海戦で鉄血の駆逐艦に敗れた。獲物に噛まれるハンター、別におかしくない……",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"3": {
						"event": "Acquisition",
						"media": "get",
						"chinese": "指挥官，你好，我是猎人。……？我还要说些什么吗？",
						"chineseTL": "Hello Commander. I'm Hunter. ...? What else did you want me to say?",
						"chineseNote": "",
						"japanese": "指揮官、こんにちは。わたしはハンター。……ほかに、言うべきことは…？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"4": {
						"event": "Login",
						"media": "login",
						"chinese": "指挥官，要和我一起去打猎吗？",
						"chineseTL": "Commander, care to join me in a hunt?",
						"chineseNote": "",
						"japanese": "指揮官、ハンティング、いい？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"5": {
						"event": "Details",
						"media": "detail",
						"chinese": "指挥官，我收集的战利品，想看吗？",
						"chineseTL": "Commander I have collected some loot, wanna take a look?",
						"chineseNote": "",
						"japanese": "指揮官、わたしの戦利品コレクション…見たい？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"6": {
						"event": "Main 1",
						"media": "main_1",
						"chinese": "这孩子？是我最重要的小伙伴哦",
						"chineseTL": "This child? It's my most important little buddy.",
						"chineseNote": "",
						"japanese": "この子？大切な仲間",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"7": {
						"event": "Main 2",
						"media": "main_2",
						"chinese": "其实我也不讨厌勇敢，但她有时候真的有点啰嗦……",
						"chineseTL": "I don't hate Hardy, but sometimes she really is naggy.",
						"chineseNote": "",
						"japanese": "ハーディのことは嫌じゃない。時々、うるさく感じるけど……",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"8": {
						"event": "Main 3",
						"media": "main_3",
						"chinese": "指挥官，一般打猎是不会穿成我这样的，这只是兴趣而已",
						"chineseTL": "Commander, most boats wouldn't dress like me, It's only my hobby.",
						"chineseNote": "",
						"japanese": "この格好は……指揮官、ハンティングする時は真似しなくていい",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"9": {
						"event": "Touch",
						"media": "touch_1",
						"chinese": "这不是猎枪，是鱼雷管",
						"chineseTL": "This isn't a hunting rifle, it's a torpedo tube.",
						"chineseNote": "",
						"japanese": "猟銃ではない、魚雷発射管",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"10": {
						"event": "Touch (Special)",
						"media": "touch_2",
						"chinese": "指挥官，Lock on……",
						"chineseTL": "Commander, Lock on...",
						"chineseNote": "",
						"japanese": "「指揮官に、ロックオン」",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"11": {
						"event": "Mission",
						"media": "task",
						"chinese": "这次的悬赏令是要猎杀什么？",
						"chineseTL": "What will you have me kill this time?",
						"chineseNote": "",
						"japanese": "今度の賞金首は…？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"12": {
						"event": "Mission Complete",
						"media": "mission_complete",
						"chinese": "怎么？难道想让我给你吹胜利号吗？",
						"chineseTL": "What? Do you want me to blow you a victory?",
						"chineseNote": "",
						"japanese": "なに？勝利のラッパを吹いてほしいと…？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"13": {
						"event": "Mail",
						"media": "mail",
						"chinese": "信件，会有狩猎情报吗？",
						"chineseTL": "Letters, will there be hunting reports?",
						"chineseNote": "",
						"japanese": "メール。ハンティングの情報はないかな？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"14": {
						"event": "Return to Port",
						"media": "home",
						"chinese": "指挥官，有战利品吗？",
						"chineseTL": "Commander, any loot?",
						"chineseNote": "",
						"japanese": "指揮官、獲物は？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"15": {
						"event": "Commission Complete",
						"media": "expedition",
						"chinese": "战斗前的准备越充分越好",
						"chineseTL": "It's best to be prepared before a battle.",
						"chineseNote": "",
						"japanese": "準備しておくに越したことはない",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"16": {
						"event": "Enhancement",
						"media": "upgrade",
						"chinese": "嗯，下次可以尝试一些大家伙了……",
						"chineseTL": "Next time, I'll try out my newly enhanced big buddy!",
						"chineseNote": "",
						"japanese": "うん、次は大きいのを狙おう……",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"17": {
						"event": "Flagship",
						"media": "warcry",
						"chinese": "你们已经跑不了了",
						"chineseTL": "Now you can't run.",
						"chineseNote": "",
						"japanese": "…逃さない",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"18": {
						"event": "Victory",
						"media": "mvp",
						"chinese": "把敌人当做猎物的话，还是挺简单的……",
						"chineseTL": "It's not so simple, treating people like game",
						"chineseNote": "",
						"japanese": "獲物だと考えていれば、獲りやすい……",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"19": {
						"event": "Defeat",
						"media": "lose",
						"chinese": "猎人与猎物的关系可不是一成不变的",
						"chineseTL": "Even the hunter could sometimes lose to the prey",
						"chineseNote": "",
						"japanese": "追う者も、追われる者も、変わらない者はいない……",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"20": {
						"event": "Skill",
						"media": "skill",
						"chinese": "越是挣扎，越是痛苦……",
						"chineseTL": "The more you struggle the more it hurts...",
