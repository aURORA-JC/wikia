					},
					"18": {
						"event": "Victory",
						"media": "mvp",
						"chinese": "好！下一个对手是谁？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "よぉし！次の相手はどいつだ？",
						"japaneseTL": "",
