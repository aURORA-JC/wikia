	"lines": {
		"skin": {
			"1": {
				"skin_id": "feinikesi",
				"id": "0",
				"label": "Phoenix",
				"dialogue": {
					"1": {
						"event": "Ship Description",
						"media": "",
						"chinese": "布鲁克林级轻巡洋舰—菲尼克斯，舷号CL-46",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ブルックリン級巡洋艦・フェニックス（CL-46）",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Brooklyn-class light cruiser – Phoenix, Hull Number CL-46!",
						"englishNote": ""
					},
					"2": {
						"event": "Biography",
						"media": "profile",
						"chinese": "我是菲尼克斯，也就是凤凰，曾经是某位大人物的座舰，无论是什么死地我都会扛过来给你看的，所以危险的任务也尽管交给我吧，不会让你失望的！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "あたしはフェニックス、鳳凰にして、とあるお方のかつての乗艦さ。名前通り、どんな死地でも凌ぎきってみせる！だからどんな危険な任務でも任せてくれ！失望はさせないよ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I'm Phoenix. I once escorted a certain important person. Just like a phoenix, no matter what happens to me, I'll always come back from death! Throw all the dangerous missions you've got at me, I won't let you down!",
						"englishNote": ""
					},
					"3": {
						"event": "Acquisition",
						"media": "get",
						"chinese": "你就是指挥官吗？我是菲尼克斯，对，就是凤凰，你能驾驱得了我吗？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "あんたが指揮官？あたしはフェニックス、そう、鳳凰のフェニックスだ。あんたはこのあたしをうまく乗りこなせるかな？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Are you the commander? I'm Phoenix. That's right, the legendary phoenix! Do you have what it takes to lead me?",
						"englishNote": ""
					},
					"4": {
						"event": "Login",
						"media": "login",
						"chinese": "Hey~指挥官！今天我会一如既往地保护你的",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "Hey！指揮官！今日もこのフェニックスが守ってやるよ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Hey, Commander! I'll protect you, like I always do!",
						"englishNote": ""
					},
					"5": {
						"event": "Details",
						"media": "detail",
						"chinese": "指挥官，你的眼神怎么有点呆滞",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官、なんかあったか？目がぼーっとしちゃって",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "What's the matter, Commander? You look kinda emotionless.",
						"englishNote": ""
					},
					"6": {
						"event": "Main 1",
						"media": "main_1",
						"chinese": "我的信条就是什么事都要做到第一！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "あたしのポリシーは、「何事も一番」だ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "My motto is: \"be the best at everything\"!",
						"englishNote": ""
					},
					"7": {
						"event": "Main 2",
						"media": "main_2",
						"chinese": "阿根廷的太阳会不会比较大呢？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "アルゼンチンの太陽はもっと大きいか？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Is the sun bigger in Argentina?",
						"englishNote": ""
					},
					"8": {
						"event": "Main 3",
						"media": "main_3",
						"chinese": "没有比死地后生更令人心跳加速的事了",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "死地を乗り切ることよりスリルなことなんてないぜ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "There's nothing more thrilling than coming back from the verge of death!",
						"englishNote": ""
					},
					"9": {
						"event": "Touch",
						"media": "touch_1",
						"chinese": "来了来了！要出海了吗？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "きたきた！そろそろ海に出るか？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "It's here, it's here! Want to head out now?",
						"englishNote": ""
					},
					"10": {
						"event": "Touch (Special)",
						"media": "touch_2",
						"chinese": "Hey~！要保持绅士！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "Hey！ちゃんとジェントルマンであってくれよ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Hey~! Be a gentleman!",
						"englishNote": ""
					},
					"11": {
						"event": "Mission",
						"media": "task",
						"chinese": "完成任务这种事怎么能落后？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "任務なら一番じゃなきゃ駄目だろ？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "How could you fall behind on something as simple as missions?",
						"englishNote": ""
					},
					"12": {
						"event": "Mission Complete",
						"media": "mission_complete",
						"chinese": "看来我们应得的奖励终于送来了",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "あたしたちのボーナス、ようやく届いたな！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Looks like our well-deserved rewards have been delivered.",
						"englishNote": ""
					},
					"13": {
						"event": "Mail",
						"media": "mail",
						"chinese": "Hey！指挥官，来新邮件了！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "へい指揮官！新しいメールだぜ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Hey! Commander! Your mail is here!",
						"englishNote": ""
					},
					"14": {
						"event": "Return to Port",
						"media": "home",
						"chinese": "Yo~看样子今天也顺利的归来了嘛！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "よっと、今日も無事帰ってきたな！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Looks like we made it back safe and sound as usual today!",
						"englishNote": ""
					},
					"15": {
						"event": "Commission Complete",
						"media": "expedition",
						"chinese": "委托完成……我说，也该让我去一趟了吧？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "委託完了（棒読み）……あのさぁ、そろそろあたしにも行かせてくれない？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Commission completed... Hey, isn't it about time you let me head out on one?",
						"englishNote": ""
					},
					"16": {
						"event": "Enhancement",
						"media": "upgrade",
						"chinese": "好！我变得更强了！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "よぉし！強くなったぞ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Alright! I've gotten stronger!",
						"englishNote": ""
					},
					"17": {
						"event": "Flagship",
						"media": "warcry",
						"chinese": "看啊，东方正在火红地燃烧！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "見よ！東方は真っ赤に燃えている！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Look to the east! It's burning red!",
						"englishNote": ""
					},
					"18": {
						"event": "Victory",
						"media": "mvp",
						"chinese": "啧啧啧，就没有更强的对手了么？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ふふ、強い相手はもういねぇか？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Hahah! Aren't there any strong enemies left for me to fight?",
						"englishNote": ""
					},
					"19": {
						"event": "Defeat",
						"media": "lose",
						"chinese": "嘁，给我记住！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ちっ、覚えてろよ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Tsk, I'll get you for this!",
						"englishNote": ""
					},
					"20": {
						"event": "Skill",
						"media": "skill",
						"chinese": "血染……东方……！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "東方は…赤く…！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Paint the East... in blood...!",
						"englishNote": ""
					},
					"21": {
						"event": "Low HP",
						"media": "hp",
						"chinese": "我的手在呼唤我握紧胜利！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "勝利をつかめとこの手が叫んでいる！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "My hand is calling out to me to seize victory!",
						"englishNote": ""
					},
					"22": {
						"event": "Affinity (Upset)",
						"media": "feeling1",
						"chinese": "胆小鬼，离我远点！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "あっちいけこの臆病者！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Get outta here, coward!",
						"englishNote": ""
					},
					"23": {
						"event": "Affinity (Stranger)",
						"media": "feeling2",
						"chinese": "只有不怕死的人才配活着！——啊指挥官你别怕，不是说真的要死要活啦，只是说心理准备，要有那样的心理准备才行",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "死を恐れないやつだけ生きる資格があるってもんだ！―――ああ指揮官怖がらないで！別に生きるとか死ぬとかじゃなく、心の準備だけはしておけって言いたかったんだ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Only those who aren't afraid of death deserve to live! Ahh, don't be afraid Commander, I don't mean that literally! What I meant to say is that it's not about living or dying, it's about what mindset you have!",
						"englishNote": ""
					},
					"24": {
						"event": "Affinity (Friendly)",
						"media": "feeling3",
						"chinese": "怎么了？如果我可以的话就跟我商量吧！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "どうした？あたしでよければ相談に乗るぜ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "What's the matter? If you're okay with me then I can give you some advice!",
						"englishNote": ""
					},
					"25": {
						"event": "Affinity (Like)",
						"media": "feeling4",
						"chinese": "虽然我十分敬仰那位大人，但是你就是你。只要你站在我的背后，我就是无敌的！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "あの方は尊敬しているけど、指揮官は指揮官だ。あたしはうしろに指揮官さえいれば無敵さ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Although I respect that man, you're still you, Commander. As long as you're with me, I'm invincible!",
						"englishNote": ""
					},
					"26": {
						"event": "Affinity (Love)",
						"media": "feeling5",
						"chinese": "原来如此，难怪和你在一起时状态好的整个人跟点燃了一样。这样啊，这就是爱啊……我以前从没体验过…",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "なるほど、指揮官といると身体が燃え上がるようなこの気持こそ、愛ってやつか……こんなの感じたことがないよ…",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I see. It's no wonder that whenever I'm with you, I'm in such high spirits! I feel like my heart is on fire! This is it, this is love... I've never experienced this before...",
						"englishNote": ""
					},
					"27": {
						"event": "Pledge",
						"media": "propose",
						"chinese": "……谢谢，我就收下了。那个……太肉麻的话我也不会说，不过既然你选择了我，那我一定不会辜负你的期待的！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "……ありがと。じゃあもらっとく。それと、あの……あたしは、甘い話とか、イチャつくとかよくわからないけど、選んでもらった以上、失望だけは絶対させないよ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "... Thank you. I'll accept it. Also, um... I don't really know how to talk all sweet or flirt or anything like that, but now that you've chosen me, I promise I won't let you down!",
						"englishNote": ""
					},
					"28": {
						"event": "Easter Egg 1 (Arizona)",
						"media": "link1",
						"chinese": "哭丧着脸可不行啊大姐头",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "姉貴、しょんぼりしちゃ駄目だぜ",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "C'mon sis, don't look so sad.",
						"englishNote": ""
					}
				}
			}
