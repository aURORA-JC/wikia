					"16": {
						"event": "Enhancement",
						"media": "upgrade",
						"chinese": "接下来要我出场了吧？OK~",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "私の出番？OK～",
						"japaneseTL": "",
