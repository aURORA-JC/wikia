					"15": {
						"event": "Commission Complete",
						"media": "expedition",
						"chinese": "在那发什么呆！和我一起去接军事委托回来的同伴！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "なにそこでぼーっとしてんのよ！委託組の出迎え、あんたも一緒に来なさいっての！",
						"japaneseTL": "",
						"japaneseNote": "",
