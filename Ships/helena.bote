	"lines": {
		"skin": {
			"1": {
				"skin_id": "hailunna",
				"id": "0",
				"label": "Helena",
				"dialogue": {
					"1": {
						"event": "Ship Description",
						"media": "",
						"chinese": "布鲁克林级轻巡洋舰—海伦娜，舷号CL-50",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ブルックリン級巡洋艦・ヘレナ（CL-50）",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Brooklyn-class Light Cruiser – Helena, Hull Number CL-50!",
						"englishNote": ""
					},
					"2": {
						"event": "Biography",
						"media": "profile",
						"chinese": "我是以不输给其他重巡姐妹们为理念设计的型号，不过果然只靠我一个人是救不了大家的……指挥官，能帮帮我吗？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ほかの重巡姉妹に負けないようにデザインされたけど、私一人の力ではみんなを救えないわ……指揮官、私を助けてもらえるかな？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I was designed to be on par with heavy cruisers, but just my power alone isn't enough to save everyone... Commander, can you help me?",
						"englishNote": ""
					},
					"3": {
						"event": "Acquisition",
						"media": "get",
						"chinese": "指挥官，您好，我是海伦娜。索敌是我的长项，请……将我们带往胜利吧",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "はじめまして、指揮官。私はヘレナ。敵を探索するのが得意なの。お願い…私たちを勝利に導いて",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Nice to meet you, Commander. I'm Helena. My strong suit is enemy reconnaissance. Please... guide us towards victory.",
						"englishNote": ""
					},
					"4": {
						"event": "Login",
						"media": "login",
						"chinese": "……啊，指挥官，欢迎回来，不好意思刚才在想事情",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "…あ、指揮官、お帰りなさい。ごめんなさい、少し考え事をしていて",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "... Ah, welcome back, Commander. Sorry, I was just brooding over something.",
						"englishNote": ""
					},
					"5": {
						"event": "Details",
						"media": "detail",
						"chinese": "光靠这些是没用的呀……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "これだけに頼ってもだめ…",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I can't rely on this thing alone...",
						"englishNote": ""
					},
					"6": {
						"event": "Main 1",
						"media": "main_1",
						"chinese": "指挥官躲起来了吗，就算这样我也知道你在哪里的",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官、隠れてるの？どこにいるかはお見通しよ",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Are you trying to hide, Commander? No matter where you hide, I know where you are.",
						"englishNote": ""
					},
					"7": {
						"event": "Main 2",
						"media": "main_2",
						"chinese": "指挥官，等战争结束以后想做点什么呢？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官、この戦争が終わったら、何をしたい？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Commander, what should we do after the war?",
						"englishNote": ""
					},
					"8": {
						"event": "Main 3",
						"media": "main_3",
						"chinese": "要是我的火力能跟雷达一样强的话……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "私の火力、このレーダーと同じぐらい強力だったらいいんだけど…",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "If only my guns could be as strong as my radar...",
						"englishNote": ""
					},
					"9": {
						"event": "Touch",
						"media": "touch_1",
						"chinese": "嗯？我没事，只是有些走神而已",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "え？私は平気よ。ただ…ぼんやりしてしまって。",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Hm? I'm fine. I'm just... a little distracted.",
						"englishNote": ""
					},
					"10": {
						"event": "Touch (Special)",
						"media": "touch_2",
						"chinese": "如果这样能让指挥官打起精神，我……是愿意的",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "これで指揮官が元気になれるなら、私は…かまわないわ",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "If this makes you feel good, then... I'm fine with it.",
						"englishNote": ""
					},
					"11": {
						"event": "Mission",
						"media": "task",
						"chinese": "让我们从任务开始一点一点累积胜利的基石吧",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "少しずつ、勝利の基盤を作っていきましょう。まずはミッション遂行ですね",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Bit by bit we'll build the foundation for victory. We'll start by doing missions.",
						"englishNote": ""
					},
					"12": {
						"event": "Mission Complete",
						"media": "mission_complete",
						"chinese": "任务的奖励累积起来也是一笔巨大的资源，请好好利用吧",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "塵も積もれば山だからね。ちゃんと利用しましょう",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Resources are no good if they just pile up. We should make use of them.",
						"englishNote": ""
					},
					"13": {
						"event": "Mail",
						"media": "mail",
						"chinese": "有新的邮件到了，请好好珍惜和其他人之间的关系哦",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "新しいメールね。よそとの付き合いも大事ね",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "You've got new mail. It's important to stay in touch with your loved ones.",
						"englishNote": ""
					},
					"14": {
						"event": "Return to Port",
						"media": "home",
						"chinese": "大家都回来了吧？感觉除了甲板上多出了点零件以外没什么大问题呢……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "みんな無事帰還したよね？でも鋼板に部品が増えている…なんだろう…",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Did everyone make it back safe and sound? Good. But there's spare parts lying around in the docks now... I wonder why...",
						"englishNote": ""
					},
					"15": {
						"event": "Commission Complete",
						"media": "expedition",
						"chinese": "指挥官，你说下次我把SG借给姐妹们出委托好不好？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官、今度はSGを姉妹たちに貸してもいい？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Hey, Commander, could I lend my SG radar to my sisters this time?",
						"englishNote": ""
					},
					"16": {
						"event": "Enhancement",
						"media": "upgrade",
						"chinese": "这份力量，我一定会用来保护大家的！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "この力、みんなを守るために使うわ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I'll use this power to protect everyone!",
						"englishNote": ""
					},
					"17": {
						"event": "Flagship",
						"media": "warcry",
						"chinese": "在我面前，你们无所遁形",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "私の前じゃ、隠れても無駄よ",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "The enemy can't hide from me.",
						"englishNote": ""
					},
					"18": {
						"event": "Victory",
						"media": "mvp",
						"chinese": "……居然赢了？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "…勝ったの！？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "... We actually won?",
						"englishNote": ""
					},
					"19": {
						"event": "Defeat",
						"media": "lose",
						"chinese": "……拜托先救我~不要再和敌人纠缠了",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "…お願い助けて…もう敵にこだわらないで…",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "... Please, save me... Ignore the enemy, just let them go...",
						"englishNote": ""
					},
					"20": {
						"event": "Skill",
						"media": "skill",
						"chinese": "SG，把你的力量借给我！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "SG、力を貸して！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "SG, give me your power!",
						"englishNote": ""
					},
					"21": {
						"event": "Low HP",
						"media": "hp",
						"chinese": "SG，锁定敌人位置！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "SG、敵の所在をロックオンよ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "SG, lock on the enemy position!",
						"englishNote": ""
					},
					"22": {
						"event": "Affinity (Upset)",
						"media": "feeling1",
						"chinese": "SG，快来保护我……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "SG、私を守って……",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "SG, protect me...",
						"englishNote": ""
					},
					"23": {
						"event": "Affinity (Stranger)",
						"media": "feeling2",
						"chinese": "我的索敌能力要是能帮到姐妹们那就好了……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "私の索敵能力、みんなの役に立てればいいのに……",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "If only my recon could help the others...",
						"englishNote": ""
					},
					"24": {
						"event": "Affinity (Friendly)",
						"media": "feeling3",
						"chinese": "表情？啊……因为我离开的时候战况还是相当不利的呢……对于和平啊，欢笑啊之类的……稍微有些不适应，不过不用担心，海伦娜没问题的！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "表情？あ……戦線から離れた時はまだ不利な状況だったから、平和とか、笑顔とか……ちょっと慣れないかも。でも心配しないで、ヘレナは大丈夫！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "My expression? Ah... it's because I left while we were on the losing end of the war... I might just not be so used to peace and happiness. But don't worry, I'll be fine!",
						"englishNote": ""
					},
					"25": {
						"event": "Affinity (Like)",
						"media": "feeling4",
						"chinese": "最近感觉放松了不少……嗯，之前神经绷的太紧了，这多亏指挥官一直在我身边给我勇气哦~",
						"chineseTL": "",
						"chineseNote": "",
