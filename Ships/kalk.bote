					"17": {
						"event": "Flagship",
						"media": "warcry",
						"chinese": "开战了开战了，要表现好点哦~",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "やるぞ～、ちゃんと頑張らなきゃね～",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "The battle has started, I have to behave myself~",
						"englishNote": ""
