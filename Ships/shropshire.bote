						"chineseNote": "",
						"japanese": "頑張らないと～",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Ganbara Knight!",
						"englishNote": ""
					},
					"21": {
						"event": "Low HP",
						"media": "hp",
						"chinese": "糟了糟了这下糟了……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ヤバイよヤバイよ……",
