					"18": {
						"event": "Victory",
						"media": "mvp",
						"chinese": "哼，就算你表扬我，我也不会轻易认同你的！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ふん！褒めても認めてやらないんだから！",
						"japaneseTL": "",
