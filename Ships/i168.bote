	"lines": {
		"skin": {
			"1": {
				"skin_id": "I168",
				"id": "0",
				"label": "I-168 ",
				"dialogue": {
					"1": {
						"event": "Ship Description",
						"media": "",
						"chinese": "海大VI型a型潜水舰伊168",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "海大VI型a伊号第百六十八潜水艦",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Kaidai Type VIa submarine – I-168.",
						"englishNote": ""
					},
					"2": {
						"event": "Biography",
						"media": "profile",
						"chinese": "我是伊号第一六八潜艇！在那场战役干掉了约克城和哈曼，给飞龙报了仇！还有…嗯…虽然我努力了…还是没击沉过什么别的船…",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "わたしは伊号第一六八潜水艦よ。あの大戦では飛龍の敵討ちで、ヨークタウンとハムマンを倒したわ。あとは…ううん……頑張ったけど……他の敵艦を沈められたことはなかったのよね…",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I am the submarine I-168. In the war, I avenged Hiryuu by taking out Yorktown and Hammann. Besides that... well... I really tried... but wasn't able to sink any other ships...",
						"englishNote": ""
					},
					"3": {
						"event": "Acquisition",
						"media": "get",
						"chinese": "我是伊168哦，叫我168就好。怎么说我也是武勋舰，要好好地让我派上用场哦！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "伊168よ。「いろは」って呼んで頂戴。わたしのようなすごい武勲艦をちゃーんと使いこなしなさい！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I'm I-168, but do call me Iroha. I expect you to know how to handle an extraordinary and decorated vessel such as myself!",
						"englishNote": ""
					},
					"4": {
						"event": "Login",
						"media": "login",
						"chinese": "太·慢·了!就算我们潜艇擅长等待时机，也不能真的让我等这么久吧…",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "おそーい！…わたしのような潜水艦が待つことは得意だからといってこんなに待たせるんじゃないわよ…",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "You're very late! ... A submarine such as myself is extremely patient, but that doesn't mean you can keep me waiting for this long...",
						"englishNote": ""
					},
					"5": {
						"event": "Details",
						"media": "detail",
						"chinese": "你找本小姐有事吧？……没事也可以……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "このわたしに用があるのよね？…用がなくても…",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "You want something from me, don't you? ... Well, even if you don't...",
						"englishNote": ""
					},
					"6": {
						"event": "Main 1",
						"media": "main_1",
						"chinese": "好无聊…指挥官，给本小姐找点乐子听见了吗！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "退屈…指揮官、わたしを楽しませなさい！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I'm bored... Entertain me, Commander!",
						"englishNote": ""
					},
					"7": {
						"event": "Main 2",
						"media": "main_2",
						"chinese": "好困啊……还是海里舒服……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "眠い……海の中のほうが快適……",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I'm tired... I like it better in the ocean...",
						"englishNote": ""
					},
					"8": {
						"event": "Main 3",
						"media": "main_3",
						"chinese": "你说哈曼？她想找茬的话就放马过来！看我不揍扁她！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ハムマン？このわたしを懲らしめたければやってみなさいな！返り討ちにしてやるわ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Hammann? If she wants to berate me then she can go ahead and try! I'll turn the tables on her!",
						"englishNote": ""
					},
					"9": {
						"event": "Touch",
						"media": "touch_1",
						"chinese": "不许随随便便就摸我！……你说按摩？是这样吗……那就交给你啦！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "気安く触らないでよ！…マッサージ？そっか……じゃ、じゃあ……頼むわね…？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Don't touch me for no reason! ... It's a massage? O-okay... Continue...",
						"englishNote": ""
					},
					"10": {
						"event": "Touch (Special)",
						"media": "touch_2",
						"chinese": "呀！？怎、怎么回事！？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ひゃう！？な、何事だぁ！？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Eeek?! W-what do you want?!",
						"englishNote": ""
					},
					"11": {
						"event": "Rub",
						"media": "touch_head",
						"chinese": "你，你干吗！？",
