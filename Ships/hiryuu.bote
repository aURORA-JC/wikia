						"japanese": "喝ッ！……うん、いいコンディション！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Hey! I’m in pretty great shape!",
						"englishNote": ""
					},
					"17": {
						"event": "Flagship",
						"media": "warcry",
						"chinese": "先手必胜！舰载机，起飞！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "先手必勝！艦載機、翔べ！",
