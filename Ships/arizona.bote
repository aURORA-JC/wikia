					"23": {
						"event": "Affinity (Stranger)",
						"media": "feeling2",
						"chinese": "啊……抱歉，我又在想些伤感的事情了……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "あぁ……すみません、また悲しいこと考えて……",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Ahh... Apologies. I was thinking about something sad again...",
						"englishNote": ""
