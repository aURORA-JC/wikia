					"22": {
						"event": "Affinity (Upset)",
						"media": "feeling1",
						"chinese": "哪里才是我的居所呢……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "私の居場所はどこなんでしょうか……",
						"japaneseTL": "",
