					"18": {
						"event": "Victory",
						"media": "mvp",
						"chinese": "各位，越是胜利的时候，越不能放松警惕",
						"chineseTL": "Everyone, the more you win, the less you should relax!",
						"chineseNote": "",
						"japanese": "みんな、勝った時だからこそ気を緩めてはいけませんよ",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "",
						"englishNote": ""
					},
					"19": {
						"event": "Defeat",
						"media": "lose",
						"chinese": "立刻撤退，不要恋战！",
						"chineseTL": "Retreat immediately! Don't stay and fight!",
						"chineseNote": "",
						"japanese": "この場に留まらないで、すぐに撤退してください！",
						"japaneseTL": "",
						"japaneseNote": "",
