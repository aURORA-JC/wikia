				"dialogue": {
					"1": {
						"event": "Ship Description",
						"media": "",
						"chinese": "奥马哈级轻巡洋舰—罗利，舷号CL-7",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "オマハ級軽巡洋艦・ローリー（CL-7)",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Omaha-class light cruiser – Raleigh, Hull Number CL-7!",
						"englishNote": ""
					},
					"2": {
						"event": "Biography",
						"media": "profile",
						"chinese": "我是奥马哈级的驱逐小队训练官——罗利，对于不听话的孩子，罗利可是很严厉的哦",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "オマハ級の駆逐戦隊の嚮導艦ーーローリーです。ローリーは言うことを聞かない子に厳しいですよ",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I'm the training officer of the Omaha-class fleet - Raleigh. I'm going to be very strict with anyone who won't listen to what I say!",
						"englishNote": ""
					},
					"3": {
						"event": "Acquisition",
						"media": "get",
						"chinese": "我是奥马哈级轻巡洋舰罗利，请收好这份契约文件……今后就要多多麻烦指挥官了",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "オマハ級軽巡洋艦のローリーです。この契約書受け取ってください。……これからよろしくお願いします。指揮官",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I'm Raleigh, the Omaha-class light cruiser. Please sign this contract here... There we go. I'm hoping we'll make a good team from now on, Commander.",
						"englishNote": ""
					},
					"4": {
						"event": "Login",
						"media": "login",
						"chinese": "你好，指挥官。罗利在这里等你很久了呢~看下时间，是时候开始训练了吧？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "こんにちは、指揮官。ローリーはずっと待っていましたよ？はい、そろそろ訓練の時間ですね？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Hello, Commander. I've been waiting for you. Oh my, look at the time! Shouldn't we start the training now?",
						"englishNote": ""
					},
					"5": {
						"event": "Details",
						"media": "detail",
						"chinese": "大家要是有什么不懂的话，要及时跟我提出来哦~",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "わからないことがあったら、先生に聞いてください",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Make sure you ask me any questions if you're lost before we head out!",
						"englishNote": ""
					},
					"6": {
						"event": "Main 1",
						"media": "main_1",
						"chinese": "需要开始训练了吗，指挥官？",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官、訓練をはじめますか？",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Shall we begin our training, Commander?",
						"englishNote": ""
					},
					"7": {
						"event": "Main 2",
						"media": "main_2",
						"chinese": "对新兵来说，每一次演习都是很宝贵的喔~",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "新米にとって、一回一回の演習はとても重要ですよ~",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "For a beginner, every bit of training is very important~!",
						"englishNote": ""
					},
					"8": {
						"event": "Main 3",
						"media": "main_3",
						"chinese": "我的名字不是LOLI，要好好地叫罗利老师哦~",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ロリじゃなくて、ちゃんとローリー先生って言ってくださーい！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "My name is Raleigh, not... that! Also, don't forget the \"Ms.\"!",
						"englishNote": ""
					},
					"9": {
						"event": "Touch",
						"media": "touch_1",
						"chinese": "别这样捏来捏去的……我又不是小孩子……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ぎゅぎゅってしないでください…もう子供じゃないです！…",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Please don't tug me like that... I'm not a kid!",
						"englishNote": ""
					},
					"10": {
						"event": "Touch (Special)",
						"media": "touch_2",
						"chinese": "罗利也是会生气的……指—挥—官！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ローリーだって怒りますよ…？しーきーかーん！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "I'm not immune to getting angry, you know... Co-man-der!",
						"englishNote": ""
					},
					"11": {
						"event": "Mission",
						"media": "task",
						"chinese": "指挥官，请记得你未完成的任务！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官、未完了の任務を忘れないでください！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Commander, please don't forget about your unfinished missions!",
						"englishNote": ""
					},
					"12": {
						"event": "Mission Complete",
						"media": "mission_complete",
						"chinese": "记得去领取任务奖励哦~",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ボーナスの受け取りは忘れないでくださいね。",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Please don't forget to claim your mission rewards.",
						"englishNote": ""
					},
					"13": {
						"event": "Mail",
						"media": "mail",
						"chinese": "新邮件已经整理好，请查收",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "新しいメールは整理しました。ご確認ください",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Your mail has been sorted. Remember to read your messages thoroughly, please!",
						"englishNote": ""
					},
					"14": {
						"event": "Return to Port",
						"media": "home",
						"chinese": "指挥官，刚才的表现不错哦！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "指揮官、さっきのはよくできました！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "You did quite well, Commander!",
						"englishNote": ""
					},
					"15": {
						"event": "Commission Complete",
						"media": "expedition",
						"chinese": "嗯，差不多时间了，指挥官，罗利去迎接委托返航的姐妹了哦",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "うん、そろそろ時間ですね。指揮官、ローリーは先に委託から帰った娘たちを迎えに行きます",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "It's almost time, Commander. I'll go ahead and welcome the girls back from their commission.",
						"englishNote": ""
					},
					"16": {
						"event": "Enhancement",
						"media": "upgrade",
						"chinese": "这样就可以更好地辅导指挥官了",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "これで指揮官をよりよく補佐できますね",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Now I can properly mentor you, Commander.",
						"englishNote": ""
					},
					"17": {
						"event": "Flagship",
						"media": "warcry",
						"chinese": "大家集中精神哟，这可是一次难得的实战呢~",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "みんな、久しぶりの実戦ですから、集中してください～",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Please pay attention, everyone! This is a good chance to get some real combat experience~",
						"englishNote": ""
					},
					"18": {
						"event": "Victory",
						"media": "mvp",
						"chinese": "同样的招式不可能对罗利用两次的~",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ローリーには同じ技は二度と通用しませんよ～",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "You can't use the same trick on me twice!",
						"englishNote": ""
					},
					"19": {
						"event": "Defeat",
						"media": "lose",
						"chinese": "呼，好险，差一点……",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "ふぅ……危なかったです……",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Phew... That was close... I nearly...",
						"englishNote": ""
					},
					"20": {
						"event": "Skill",
						"media": "skill",
						"chinese": "让我来演示一下教科书上的动作吧",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "教科書通りの動きを実演してみせますよ",
						"japaneseTL": "",
						"japaneseNote": "",
