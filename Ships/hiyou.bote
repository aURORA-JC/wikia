					"20": {
						"event": "Skill",
						"media": "skill",
						"chinese": "舰载机……升空！",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "艦載機…発進せよ！",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "Planes... take off!",
						"englishNote": ""
					},
					"21": {
						"event": "Affinity (Upset)",
						"media": "feeling1",
						"chinese": "肮脏——不、我不是在说你哦",
						"chineseTL": "",
						"chineseNote": "",
						"japanese": "汚らわしい……はぁ？お前のことじゃない…",
						"japaneseTL": "",
						"japaneseNote": "",
						"english": "So filthy... What? I'm not talking about you...",
						"englishNote": ""
